{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["es2022"],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".vercel/output",
    "moduleResolution": "node"
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
}
